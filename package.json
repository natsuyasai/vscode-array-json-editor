{
  "name": "array-json-editor",
  "displayName": "array-json-editor",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "array-json-editor.openEditor",
        "displayName": "Open Array JSON Editor",
        "selector": [
          {
            "filenamePattern": "*.array.json"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "array-json-editor.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "array-json-editor.openEditor",
        "title": "Open Array JSON Editor"
      }
    ]
  },
  "scripts": {
    "install:all": "npm install && cd webview-ui && npm install",
    "start:webview": "cd webview-ui && npm run start",
    "build:webview": "cd webview-ui && npm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.5.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}